programa {
  funcao inicio() {
    cadeia login 
    cadeia senha

    escreva("Digite o seu login: ")
    leia(login)
    escreva("Digite a sua senha: ")
    leia(senha)

     se (login == "admin" e senha == "1234") {
      escreva ("Seja bem-vindo!")
    } senao {
      escreva ("Dados invalidos")
    }


  }
}
